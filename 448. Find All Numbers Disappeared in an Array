class Solution:
    def findDisappearedNumbers(self, nums: List[int]) -> List[int]:
        res = []
        for i in range(1,len(nums)+1):
            if i not in nums:
                res.append(i)
        return res

--------------------------
without brute force :)
--------------------------

class Solution:
    def findDisappearedNumbers(self, nums: List[int]) -> List[int]:
        for num in nums:
            index = abs(num) - 1
            nums[index] = -abs(nums[index])  # mark as seen

        return [i + 1 for i in range(len(nums)) if nums[i] > 0]

        
